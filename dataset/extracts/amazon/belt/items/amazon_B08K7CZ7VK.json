            },
            {
                "reviewerName": "bennylog",
                "reviewerLink": "https://www.amazon.com/gp/profile/amzn1.account.AFFWNYXHBNCJXJHT7APR5WSZDINA",
                "reviewLink": "https://www.amazon.com/gp/customer-reviews/R1RNHDLURW2VCC?ASIN=B0CST37ZXR",
                "reviewRating": "5.0 out of 5 stars",
                "reviewDate": "Reviewed in the United States on June 13, 2023",
