            {
                "reviewerName": "amys",
                "reviewerLink": "https://www.amazon.com/gp/profile/amzn1.account.AEQCETKSRLR3SLIPFGFUDNEDMRNQ",
                "reviewLink": "https://www.amazon.com/gp/customer-reviews/R3SEVUSFYCYVDS?ASIN=B0CQZYSZSJ",
                "reviewRating": "4.0 out of 5 stars",
                "reviewDate": "Reviewed in the United States on January 4, 2024",
                "reviewTitle": "4.0 out of 5 stars Almost perfect, very comfy",
